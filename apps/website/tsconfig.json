{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
      "@packages/*": ["../../packages/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}
